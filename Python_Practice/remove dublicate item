#Count the number of reoccured element
#remove dublicate element

fruits = ["Apple", "Cherry", "Mango","Banana","Apple", "Cherry",  "Cherry", "Mango" ,"Apple","Apple"]
fr = fruits.count("Apple")
print(fr)
rem = fruits.clear()
def remove_dublicate(input_list):
    unique_list =[]
    for item in input_list:
        if item not in unique_list:
            unique_list.append(item)
    return unique_list

fruits = ["Apple", "Cherry", "Mango","Banana","Apple", "Cherry",  "Cherry", "Mango" ,"Apple","Apple"]
new_list = remove_dublicate(fruits)
print("The old list was /n " , fruits)
print("The new list is /n", new_list)

